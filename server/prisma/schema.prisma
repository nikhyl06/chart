datasource db {
  provider = "postgresql"
  url      = env("DATABASE_URL")
}

generator client {
  provider = "prisma-client-js"
}

model StockData {
  id      Int      @id @default(autoincrement())
  co_code String
  date    DateTime
  open    Float
  high    Float
  low     Float
  close   Float
  mcap    Float
  volume  Int
}